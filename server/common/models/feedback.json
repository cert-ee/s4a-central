{
  "name": "feedback",
  "plural": "feedbacks",
  "base": "PersistedModel",
  "properties": {
    "case_number": {
      "type": "string",
      "required": false
    },
    "message": {
      "type": "string",
      "description": "Autogenerated message",
      "required": true
    },
    "comment": {
      "type": "string",
      "required": false,
      "description": "Comment from Detector",
      "default": ""
    },
    "machine_id": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "internal_comment": {
      "type": "string",
      "required": false,
      "description": "Central comment"
    },
    "logs": {
      "type": "object",
      "required": false,
      "description": "Logs",
      "default": {}
    },
    "system_info": {
      "type": "object",
      "required": false,
      "description": "System information",
      "default": {}
    },
    "network_interfaces": {
      "type": "array",
      "required": false,
      "description": "Network interfaces",
      "default": []
    },
    "contacts": {
      "type": "object",
      "required": false,
      "description": "Available registration information",
      "default": {}
    },
    "components": {
      "type": "array",
      "required": false,
      "description": "Components information from detector",
      "default": []
    },
    "solved": {
      "type": "boolean",
      "required": false,
      "description": "If the case is solved by Cert",
      "default": false
    },
    "modified_time": {
      "type": "date",
      "required": true,
      "description": "Modified time",
      "default": "$now"
    },
    "created_time": {
      "type": "date",
      "required": true,
      "description": "Created time",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "detector": {
      "type": "belongsTo",
      "model": "detector",
      "foreignKey": "detectorId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "read",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
